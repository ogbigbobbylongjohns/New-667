-- **Animations Page**
local Animations = UI:addPage("ğŸƒ Animations", 2, false, 6)
Animations:addLabel("ğŸƒ Animations", "All Animation Features")

local animationTracks = {}

local function toggleAnimation(state, animationName)
    local player = game.Players.LocalPlayer
    local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end

    local animator = humanoid:FindFirstChildOfClass("Animator") or Instance.new("Animator", humanoid)

    if state then
        for _, anim in ipairs(workspace:GetDescendants()) do
            if anim:IsA("Animation") and anim.Name:lower():find(animationName) then
                local track = animator:LoadAnimation(anim)
                track:Play()
                animationTracks[animationName] = track
                return
            end
        end
    else
        if animationTracks[animationName] then
            animationTracks[animationName]:Stop()
            animationTracks[animationName]:Destroy()
            animationTracks[animationName] = nil
        end
    end
end

Animations:addToggle("ğŸ Snake Crawl", function(state) toggleAnimation(state, "snake crawl") end)
Animations:addToggle("ğŸ”’ Handcuff Animation", function(state) toggleAnimation(state, "cuffed") end)
Animations:addToggle("ğŸ›‘ Crouch Animation", function(state) toggleAnimation(state, "crouch") end)

